:-not p(X),not t_0(MAX_a),max_a(X),s_0(X).
:-not t_0(MAX_a),max_a(X),s_0(X),q(X).
t_0(X):-not p(NV_0),succ_a(X,NV_0).
t_0(X):-q(NV_0),succ_a(X,NV_0).
p(NV_0)|t_0(X):-succ_a(X,NV_0),q(NV_0).
:-not p(NV_0),not t_0(X),succ_a(X,NV_0).
:-not t_0(X),succ_a(X,NV_0),q(NV_0).
%NEW variable define
_p(X):- not p(X).
